<IfModule mod_rewrite.c>
    RewriteEngine On

    # Si le fichier ou dossier demand√© existe, ne rien faire
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d

    # Sinon, rediriger vers index.php (Symfony front controller)
    RewriteRule ^ index.php [QSA,L]
</IfModule>
